
/ITL/scene/turenas xorigin 0.0;
/ITL/scene/turenas x 0.0;
/ITL/scene/_cursor del;

# resynchronize the cursor.
/ITL/scene/sync cursor turenas v;
/ITL/scene/sync cursorsmall;
/ITL/scene/cursorsmall show 0;

/ITL/scene/_smallturenas show 0;

# reload the watched events
/ITL load "scripts/watch.inscore";
