/ITL/scene/* del ;
/ITL/scene width 1.5 ;

w = 0.12;
/ITL/scene/score set "gmnf" "rsrc/bach.gmn" ;
/ITL/scene/score columns 2 ;
/ITL/scene/score scale 0.35 ;
/ITL/scene/score map voice1 ;
/ITL/scene/score map voice2 ;
/ITL/scene/score map voice3 ;
/ITL/scene/spot1 set ellipse $w $w ;
/ITL/scene/spot1 color 0 0 240 160 ;
/ITL/scene/spot* date 3 64 ;
/ITL/scene/sync spot1 "score:voice1" ;
/ITL/scene/sync spot1 "score:voice2" ;
/ITL/scene/sync spot1 "score:voice3" ;
